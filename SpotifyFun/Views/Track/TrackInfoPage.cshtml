@{
    dynamic trackJSON = TempData["trackJSON"];
    string title = trackJSON.name;
    string previewURL = trackJSON.preview_url;
    string artist = trackJSON.artists[0].name;
    string trackURI = trackJSON.uri.ToString();

    string url = string.Format("https://open.spotify.com/embed?uri={0}&view={1}", trackURI, "cover");
   
    ViewBag.Title = @title;
}

<script src="@Url.Content("../../Scripts/dynamicElements.js")" type="text/javascript"></script>

<div id="trackInfo">
    <h1>@title</h1>
    <h2>@Html.ActionLink(@artist, "SearchArtistByName", "Artist", new { artistName = @artist }, null)</h2>
    <iframe src=@url width="500" height="640" frameborder="0" allowtransparency="true"></iframe>
    
</div>

